                                                </AttributeRule>
                                                <AttributeRule AttributeName="Scale" />
                                              </AttributeRules>
                                            </EntityRule>
                                            <EntityRule EntityName="IfcCartesianTransformationOperator3DnonUniform">
                                              <AttributeRules>
                                                <AttributeRule AttributeName="Scale2" />
