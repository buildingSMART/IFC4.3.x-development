<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ifdguid="180c9380d1c711e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4x3_RC2" />
  <Name>Pset_CableSegmentTypeBusBarSegment</Name>
  <Definition>Properties specific to busbar cable segments.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcCableSegment/BUSBARSEGMENT</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcCableSegment/BUSBARSEGMENT</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="1e027480d1c711e1800000215ad4efdf">
      <Name>IsHorizontalBusbar</Name>
      <Definition>Indication of whether the busbar occurrences are routed horizontally (= TRUE) or vertically (= FALSE).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Is Horizontal Busbar</NameAlias>
        <NameAlias lang="ja-JP">水平母線</NameAlias>
        <NameAlias lang="ko-KR">수평 모선</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">母線は、水平方向にルーティングされるかどうかを示す（= TRUE）または垂直方向に（= FALSE）を返します。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">모선은 가로로 라우팅되는지 여부를 나타내는 (= TRUE) 또는 수직 (= FALSE)를 반환합니다.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">busbarケーブルの性質、性能。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
