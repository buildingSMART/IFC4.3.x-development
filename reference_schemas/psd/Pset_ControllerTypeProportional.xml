      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ko-KR">미분 게인 (Kd)</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="f236bd00d1cd11e1800000215ad4efdf">
