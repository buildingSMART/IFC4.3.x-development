      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">あらかじめ定義済みのスプリンクラーの型から必要な型を設定する識別。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="31bdd300d1e711e1800000215ad4efdf">
      <Name>Activation</Name>
      <Definition>Identifies the predefined methods of sprinkler activation from which that required may be set.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_SprinklerActivation">
            <EnumItem>BULB</EnumItem>
            <EnumItem>FUSIBLESOLDER</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>BULB</Name>
              <NameAliases>
                <NameAlias lang="en">Bulb</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>FUSIBLESOLDER</Name>
              <NameAliases>
                <NameAlias lang="en">Fusible Solder</NameAlias>
              </NameAliases>
              <DefinitionAliases />
