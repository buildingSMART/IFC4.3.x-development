<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ifdguid="181c8b80d1e711e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4x3_RC2" />
  <Name>Pset_FireSuppressionTerminalTypeSprinkler</Name>
  <Definition>Device for sprinkling water from a pipe under pressure over an area (BS6100 100 3432)</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcFireSuppressionTerminal/SPRINKLER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcFireSuppressionTerminal/SPRINKLER</ApplicableTypeValue>
  <PropertyDefs>
