      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_SprinklerResponse">
            <EnumItem>QUICK</EnumItem>
            <EnumItem>STANDARD</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>QUICK</Name>
              <NameAliases>
                <NameAlias lang="en">Quick</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>STANDARD</Name>
              <NameAliases>
                <NameAlias lang="en">Standard</NameAlias>
              </NameAliases>
              <DefinitionAliases />
