            <ConstantDef>
              <Name>SIDEWALL</Name>
              <NameAliases>
                <NameAlias lang="en">Sidewall</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>UPRIGHT</Name>
              <NameAliases>
                <NameAlias lang="en">Upright</NameAlias>
