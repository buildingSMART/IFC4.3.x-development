            <ConstantDef>
              <Name>WASHFOUNTAIN</Name>
              <NameAliases>
                <NameAlias lang="en">Wash Fountain</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>WASHINGTROUGH</Name>
