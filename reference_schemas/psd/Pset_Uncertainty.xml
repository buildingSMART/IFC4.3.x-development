      <Definition>Indication of the basis of the uncertainty</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_UncertaintyBasis">
            <EnumItem>MEASUREMENT</EnumItem>
            <EnumItem>OBSERVATION</EnumItem>
            <EnumItem>ASSESSMENT</EnumItem>
            <EnumItem>INTERPRITATION</EnumItem>
            <EnumItem>ESTIMATE</EnumItem>
            <EnumItem>USERDEFINED</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>NOTDEFINED</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>MEASUREMENT</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>OBSERVATION</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>ASSESSMENT</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>INTERPRITATION</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>ESTIMATE</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>USERDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">User Defined</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <Definition>Value is unkown</Definition>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
              </DefinitionAliases>
