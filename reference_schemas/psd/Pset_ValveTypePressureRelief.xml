<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ifdguid="6779b080d21511e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4x3_RC2" />
  <Name>Pset_ValveTypePressureRelief</Name>
  <Definition>Spring or weight loaded valve that automatically discharges to a safe place fluid that has built up to excessive pressure in pipes or fittings.
Note that a pressure relief valve is constrained to have a single port pattern.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcValve/PRESSURERELIEF</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcValve/PRESSURERELIEF</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="6cd6fb00d21511e1800000215ad4efdf">
      <Name>ReliefPressure</Name>
      <Definition>The pressure at which the spring or weight in the valve is set to discharge fluid.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Relief Pressure</NameAlias>
        <NameAlias lang="ja-JP">リリーフ圧力</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">バルブのバネやおもりが流体を放出する作動するときの圧力。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">リリーフ弁(逃がし弁)</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>