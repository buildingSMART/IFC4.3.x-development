            <ConstantDef>
              <Name>CAST_IN_PLACE</Name>
              <NameAliases>
                <NameAlias lang="en">Cast In Place</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
