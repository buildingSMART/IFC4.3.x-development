      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">許容動作周囲温度範囲</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="83189300d1e411e1800000215ad4efdf">
      <Name>NominalAirFlowRate</Name>
