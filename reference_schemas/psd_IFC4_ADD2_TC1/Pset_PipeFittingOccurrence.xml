<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="c417a400d1f211e1800000215ad4efdf" templatetype="PSET_OCCURRENCEDRIVEN" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd" xmlns="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_PipeFittingOccurrence</Name>
  <Definition>Pipe segment occurrence attributes attached to an instance of IfcPipeSegment.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcPipeFitting</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcPipeFitting</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="c974ee80d1f211e1800000215ad4efdf">
      <Name>InteriorRoughnessCoefficient</Name>
      <Definition>The interior roughness coefficient of the pipe segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Interior Roughness Coefficient</NameAlias>
        <NameAlias lang="ja-JP">内部粗度係数</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">配管部の内部粗度係数</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="d0036600d1f211e1800000215ad4efdf">
      <Name>Color</Name>
      <Definition>The color of the pipe segment.

Note: This is typically used only for plastic pipe segments. However, it may be used for any pipe segments with a painted surface which is not otherwise specified as a covering.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Color</NameAlias>
        <NameAlias lang="ja-JP">色</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">配管部の内部粗度係数　記：プラスチック配管にのみ使われる。保護のためではない塗装された表面を持つ配管にも使われる。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
  </PsetDefinitionAliases>
</PropertySetDef>