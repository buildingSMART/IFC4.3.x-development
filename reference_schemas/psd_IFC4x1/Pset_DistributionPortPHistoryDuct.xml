      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries" />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Wet Bulb Temperature</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
