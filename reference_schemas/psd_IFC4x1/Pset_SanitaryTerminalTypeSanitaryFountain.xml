        <DefinitionAlias lang="ja-JP">要素から排水溝への下水管サイズ。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
