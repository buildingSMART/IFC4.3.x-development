      <Name>PlasticShapeFactorY</Name>
      <Definition>Ratio of plastic versus elastic bending moment capacity about the section analysis axis ys. A dimensionless value.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Plastic Shape Factor Y</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="25417680d1f711e1800000215ad4efdf">
      <Name>PlasticShapeFactorZ</Name>
      <Definition>Ratio of plastic versus elastic bending moment capacity about the section analysis axis zs. A dimensionless value.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Plastic Shape Factor Z</NameAlias>
      </NameAliases>
